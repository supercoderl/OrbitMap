{
  "expo": {
    "name": "OrbitMap",
    "slug": "OrbitMap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/zeaflo-logo-with-text.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/zeaflo-logo-with-text.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAvEp9mAH6BS0FHWYKykfhCR5IrsyaDJKI"
        }
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.supercoderle.orbitmap"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/zeaflo-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/zeaflo-logo-with-text.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Cho phép $(PRODUCT_NAME) truy cập vào máy ảnh của bạn",
          "microphonePermission": "Cho phép $(PRODUCT_NAME) truy cập vào micro của bạn",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c4d0537f-7eb2-48b1-b93d-07b2d1b82e30"
      }
    }
  }
}
